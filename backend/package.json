{
  "name": "@tsed/passportjs-example",
  "version": "1.0.0",
  "description": "Here an example project with Passport.js and Ts.ED framework.",
  "main": "src/index.js",
  "scripts": {
    "clean": "rimraf '{src,test}/**/*.{js,js.map}'",
    "build": "npm run tsc",
    "test": "npm run clean && npm run test:lint && npm run test:coverage",
    "test:unit": "cross-env NODE_ENV=test mocha",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha",
    "test:lint": "tslint --project tsconfig.json",
    "test:lint:fix": "tslint --project tsconfig.json --fix",
    "travis:deploy-once": "travis-deploy-once",
    "travis:coveralls": "nyc report --reporter=text-lcov | coveralls",
    "tsc": "tsc --project tsconfig.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start:dev": "nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\" --exec ts-node src/index.ts",
    "start": "ts-node src/index.ts"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tsed/ajv": "5.44.6",
    "@tsed/common": "5.44.6",
    "@tsed/core": "5.44.6",
    "@tsed/di": "5.44.6",
    "@tsed/passport": "5.44.6",
    "@tsed/swagger": "5.44.6",
    "@tsed/testing": "5.44.6",
    "@types/swagger-schema-official": "2.0.20",
    "ajv": "6.12.0",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "concurrently": "5.1.0",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "express": "4.17.1",
    "express-session": "1.17.0",
    "method-override": "3.0.0",
    "node-uuid": "1.4.8",
    "passport": "0.4.1",
    "passport-http": "^0.3.0",
    "passport-local": "1.0.0",
    "ts-json-properties": "2.0.3"
  },
  "devDependencies": {
    "@types/chai": "4.2.10",
    "@types/chai-as-promised": "7.1.2",
    "@types/express": "4.17.3",
    "@types/http-proxy": "1.17.3",
    "@types/mocha": "5.2.7",
    "@types/node": "13.9.0",
    "@types/passport": "1.0.2",
    "@types/passport-local": "1.0.33",
    "@types/request-promise": "4.1.45",
    "@types/sinon": "7.5.2",
    "@types/sinon-chai": "3.2.3",
    "@types/supertest": "2.0.8",
    "@types/cors": "2.8.6",
    "cross-env": "6.0.3",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "mocha": "6.2.2",
    "nodemon": "2.0.2",
    "nyc": "14.1.1",
    "rimraf": "3.0.2",
    "sinon": "7.5.0",
    "sinon-chai": "3.5.0",
    "supertest": "4.0.2",
    "ts-node": "8.6.2",
    "tslint": "5.20.1",
    "typescript": "3.8.3"
  }
}
